#THIS IS MY FIRST PROGRAM GIT.
git init

upload
git add fileName
git commit -m 'message'
git push origin master

download
git clone https://github.com/username/project.git 
git remote add upstream https://github.com/username/project.git
git fetch upstream
git merge upstream/master

create branch
git branch mybranch
git checkout -b mybranch = git branch xx git checkout xxx
git merge mybranch(back into master)
